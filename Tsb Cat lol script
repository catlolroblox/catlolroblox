coroutine.resume(coroutine.create(function()
	while task.wait() do
		for i,v in pairs(game.Workspace.Live:GetChildren()) do
			if v.ClassName == "Model" and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") then
				for p,a in pairs(v:GetChildren()) do
					if a.Name:find("Counter") then
					if not v:FindFirstChild("ESPPLAYERULTLOL") then
						local va = Instance.new("BoolValue")
						va.Name = "ESPPLAYERULTLOL"
						va.Parent = v
						local highlightplr = Instance.new("Highlight")
						highlightplr.Parent = v
						highlightplr.Enabled = true
						highlightplr.FillColor = Color3.new(0, 0, 0.498039)
						coroutine.resume(coroutine.create(function()
							repeat wait() until not v:FindFirstChild(a)
							highlightplr:Destroy()
							va:Destroy()
						end))
						end
					end
				end
				if v:FindFirstChildOfClass("ForceField") then
					if not v:FindFirstChild("ESPPLAYERFFLOL") then
						local va = Instance.new("BoolValue")
						va.Name = "ESPPLAYERFFLOL"
						va.Parent = v
						local highlightplr = Instance.new("Highlight")
						highlightplr.Parent = v
						highlightplr.Enabled = true
						highlightplr.FillColor = Color3.new(1, 1, 1)
						coroutine.resume(coroutine.create(function()
							repeat wait() until not v:FindFirstChildOfClass("ForceField")
							highlightplr:Destroy()
							va:Destroy()
						end))
					end
				end
				if not v:FindFirstChild("ESPPLAYERLOL") then
					if v.Name == "Crab Boss" then
						local va = Instance.new("BoolValue")
						va.Name = "ESPPLAYERLOL"
						va.Parent = v
						local highlightplr = Instance.new("Highlight")
						highlightplr.Parent = v
						highlightplr.Enabled = true
						highlightplr.FillColor = Color3.new(0.333333, 1, 0)
						local espthinglol = Instance.new("Beam")
						local e = Instance.new("Attachment")
						local z = Instance.new("Attachment")
						espthinglol.Name = "ESPTHING"
						espthinglol.Brightness = 1
						espthinglol.Color = ColorSequence.new(Color3.new(1, 0, 0),Color3.new(1, 0, 0))
						espthinglol.Attachment1 = e
						espthinglol.Attachment0 = z
						espthinglol.Width0 = 0.3
						espthinglol.Width1 = 0.3
						v.HumanoidRootPart.Transparency = 0
						e.Parent = v.HumanoidRootPart
						z.Parent = plr.Character.HumanoidRootPart
						espthinglol.Parent = plr.Character
						espthinglol.FaceCamera = true
					end
					if v.Name == target and v:FindFirstChild("HumanoidRootPart") then
						local va = Instance.new("BoolValue")
						va.Name = "ESPPLAYERLOL"
						va.Parent = v
						v.HumanoidRootPart.Transparency = 0
						local highlightplr = Instance.new("Highlight")
						highlightplr.Parent = v
						highlightplr.Enabled = true
						highlightplr.FillColor = Color3.new(0.333333, 1, 0)
						local espthinglol = Instance.new("Beam")
						local e = Instance.new("Attachment")
						local z = Instance.new("Attachment")
						espthinglol.Name = "ESPTHING"
						espthinglol.Brightness = 1
						espthinglol.Color = ColorSequence.new(Color3.new(0.333333, 1, 0),Color3.new(0.333333, 1, 0))
						espthinglol.Attachment1 = e
						espthinglol.Attachment0 = z
						espthinglol.Width0 = 0.3
						espthinglol.Width1 = 0.3
						e.Parent = v.HumanoidRootPart
						z.Parent = plr.Character.HumanoidRootPart
						espthinglol.Parent = plr.Character
						espthinglol.FaceCamera = true
						coroutine.resume(coroutine.create(function()
							repeat wait() until target ~= v.Name
							highlightplr:Destroy()
							espthinglol:Destroy()
							va:Destroy()
							e:Destroy()
							z:Destroy()
						end))
					end
				end
			end
		end
	end
end))
function getplayerlist()
	print("Getting playerlist")
	if close == true then return end
	for i,v in pairs(playerlistframe:GetChildren()) do
		if v.ClassName == "TextButton" then
			v:Destroy()
		end
	end
	
	for i,v in pairs(game.Players:GetChildren()) do
		if v.Name ~= plr.Name then
		local button = Instance.new("TextButton")
			button.Parent = playerlistframe
			button.Text = v.Name
			if not v.Character then
				print("player "..v.Name.." Failed to find his character bro what the")
			end
			if v.Character then
			if v.Character:FindFirstChild("Humanoid") then
		       button.Text = v.Character.Humanoid.DisplayName.." ("..v.Name..")"
			end	
			if not v.Character:FindFirstChild("Humanoid") then
				print("player "..v.Name.." Failed to find humanoid")
				end
			end
		button.Name = "Player"..i
		button.TextScaled = true
		button.MouseButton1Click:Connect(function()
			target = v.Name
				print("Select "..v.Name)
				local sound = Instance.new("Sound")
				sound.SoundId = "rbxassetid://9119713951"
				sound.Volume = 2
				sound.Parent = plr.PlayerGui
				sound:Play()
				game.Debris:AddItem(sound,5)
			end)
		end
	end
	
end
local afk = false
local autododgetoggle = false
UIS.InputBegan:Connect(function(key ,typing)
	if typing == true then return end
	if key.KeyCode == Enum.KeyCode.V then
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://12221967"
		sound.Volume = 3
		sound.Parent = plr.PlayerGui
		sound:Play()
		game.Debris:AddItem(sound,5)
		autododgetoggle = not autododgetoggle
		if autododgetoggle == true then
			autododge.Text = "AUTO DODGE (V) [ON]"
		else
			autododge.Text = "AUTO DODGE (V) [OFF]"
		end
	end
end)
local p = false
function autododgelol()
	local rs =game:GetService("RunService")
	rs.RenderStepped:Connect(function()
	if close == true then return end
			if not plr.Character then return end
			if not plr.Character:FindFirstChild("HumanoidRootPart") then return end
			if autododgetoggle == false then return end
		if plr.Character.HumanoidRootPart.Velocity.Y < -200 then
				if oldpos ~= nil then
					plr.Character.HumanoidRootPart.CFrame = oldpos
					local sound = Instance.new("Sound")
					sound.SoundId = "rbxassetid://5066021887"
					sound.Volume = 0.5
					sound.Parent = plr.PlayerGui
					sound:Play()
					game.Debris:AddItem(sound,5)
				end
		end
    end)
local autododgehitbox = Instance.new("Part")
autododgehitbox.Transparency = 1
autododgehitbox.Size = Vector3.new(100,500,100)
autododgehitbox.CanCollide = false
	autododgehitbox.Color = Color3.new(1, 1, 1)
	coroutine.resume(coroutine.create(function()
		repeat wait() until close == true or plr.Character.Humanoid.Health <= 0
		autododgehitbox:Destroy()
	end))
autododgehitbox.CanTouch = true
	autododgehitbox.Anchored = false
	autododgehitbox.CastShadow = false
local push = Instance.new("BodyVelocity")
push.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
	push.Velocity = Vector3.new(0,0,0)
push.Parent = autododgehitbox
autododgehitbox.Parent = game.Workspace
local autododgecd = false
	local autododgecd2 = false
local autododgecd3 = false
coroutine.resume(coroutine.create(function()
	while wait() do
		if close == true then break end
		if not autododgehitbox then return end
		if not plr.Character then return end
		if not plr.Character:FindFirstChild("HumanoidRootPart") then return end
		if autododgetoggle == true then
			autododgehitbox.Transparency = 0.75
			autododgehitbox.CFrame = plr.Character.HumanoidRootPart.CFrame
				push.Velocity = Vector3.new(0,10,0)
			else
				autododgehitbox.Transparency = 0.9
				autododgehitbox.CFrame = CFrame.new(1073.91, 300, 23000.9)
				push.Velocity = Vector3.new(0,0,0)
			end
	end
end))
autododgehitbox.Touched:Connect(function(hit)
	if close == true then return end
	if autododgetoggle == false then return end
	if autododgecd == true then return end
		if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= plr.Name and hit.Parent:FindFirstChild("HumanoidRootPart") then
			if hit.Parent.Humanoid.FloorMaterial == Enum.Material.Air then return end
			plr.Character.HumanoidRootPart.CFrame = hit.Parent.HumanoidRootPart.CFrame * CFrame.new(0,0,105)
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://5066021887"
		sound.Volume = 0.5
		sound.Parent = plr.PlayerGui
		sound:Play()
		game.Debris:AddItem(sound,5)
		coroutine.resume(coroutine.create(function()
			autododgecd = true
			task.wait(1)
			autododgecd = false
			end))
			for i,v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "GreenAura" or v.Name == "GlowingArm" or v.Name == "HasKatana" then
					local vim = game:GetService("VirtualInputManager")
					vim:SendKeyEvent(true, Enum.KeyCode.F, false, nil)
					coroutine.resume(coroutine.create(function()
						repeat task.wait() until not hit.Parent:FindFirstChild(v)
						vim:SendKeyEvent(false, Enum.KeyCode.F, false, nil)
					end))
				end
			end
	end
end)
