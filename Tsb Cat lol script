targethim.MouseButton1Click:Connect(function()
	if game.Players:FindFirstChild(target) then
		mainframe.Target.Value = target
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://7433801607"
		sound.Volume = 2
		sound.Parent = plr.PlayerGui
		sound:Play()
		game.Debris:AddItem(sound,5)
		print("Target SUCCESS "..target)
	else
		print("failed to target")
	end
end)
function afkmodern()
coroutine.resume(coroutine.create(function()
	while task.wait() do
		if close == true then break end
		if not plr.Character then return end
		if not plr.Character:FindFirstChild("HumanoidRootPart") then return end
		if afk == true and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") and oldpos ~= nil then
			plr.Character.HumanoidRootPart.CFrame = oldpos
			local bodyvelocity = Instance.new("BodyVelocity")
			bodyvelocity.Parent = plr.Character.HumanoidRootPart
			bodyvelocity.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			bodyvelocity.Velocity = Vector3.new(0,2.5,0)
			game.Debris:AddItem(bodyvelocity, 1)
		end
	end
	end))
end
afkmodern()
getplayerlist()
game.Players.ChildAdded:Connect(function()
	if close == true then return end
	getplayerlist()
end)
game.Players.ChildRemoved:Connect(function()
	if close == true then return end
	getplayerlist()
end)
function cdfuc(len)
	if close == true then return end
	coroutine.resume(coroutine.create(function()
	cd = true
	wait(len)
	cd = false
	end))
end
