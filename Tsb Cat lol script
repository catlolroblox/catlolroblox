for i,v in pairs(buttonlistframe2:GetChildren()) do
	if v.ClassName == "TextBox" then
		v.BackgroundTransparency = 0.75
		v.TextColor3 = Color3.new(1, 1, 1)
		v.TextScaled = true
		v.Parent = buttonlistframe2
		v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
	end
	if v.ClassName == "TextButton" then
		v.BackgroundTransparency = 0.75
		v.BackgroundColor3 = Color3.new(0.333333, 1, 0)
		v.TextScaled = true
		v.Parent = buttonlistframe2
		v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
		local canclick = true
		v.MouseButton1Click:Connect(function()
			if canclick == true then
				local sound = Instance.new("Sound")
				sound.SoundId = soundva.SoundId
				sound.Volume = 2
				sound.Parent = game.Players.LocalPlayer.PlayerGui
				sound:Play()
				game.Debris:AddItem(sound,5)
			end
		end)
		coroutine.resume(coroutine.create(function()
			repeat wait() until savechange == true
			canclick = false
		end))
	end
end
