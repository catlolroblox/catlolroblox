buttonsave.MouseButton1Click:Connect(function()
	EditThemeFrame.Visible = false
	tc1va = buttontc1.Text
	tc2va = buttontc2.Text
	tc3va = buttontc3.Text
	bgc1va = buttonbc1.Text
	bgc2va = buttonbc2.Text
	bgc3va = buttonbc3.Text
	themeid = "rbxassetid://"..buttontheme.Text
	soundva.SoundId = "rbxassetid://"..buttonsound.Text
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://7148585764"
	sound.Volume = 2
	sound.Parent = game.Players.LocalPlayer.PlayerGui
	sound:Play()
	game.Debris:AddItem(sound,5)
	savechange = true
	wait(1)
	savechange = false
	mainframe.Image = themeid
	mainframe2.Image = themeid
	mainframe.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
	mainframe2.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
	for i,v in pairs(buttonlistframe:GetChildren()) do
		if v.ClassName == "TextLabel" or v.ClassName == "TextBox" then
			v.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
			v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
		end
		if v.ClassName == "TextButton" then
			local canclick = true
			v.MouseButton1Click:Connect(function()
				if canclick == true then
				local sound = Instance.new("Sound")
				sound.SoundId = soundva.SoundId
				sound.Volume = 2
				sound.Parent = game.Players.LocalPlayer.PlayerGui
				sound:Play()
				game.Debris:AddItem(sound,5)
				end
			end)
			v.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
			v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
			coroutine.resume(coroutine.create(function()
				repeat wait() until savechange == true
                canclick = false
			end))
		end
	end
	for i,v in pairs(buttonlistframe2:GetChildren()) do
		if v.ClassName == "TextLabel" or v.ClassName == "TextBox" then
			v.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
			v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
		end
		if v.ClassName == "TextButton" then
			local canclick = true
			v.MouseButton1Click:Connect(function()
				if canclick == true then
					local sound = Instance.new("Sound")
					sound.SoundId = soundva.SoundId
					sound.Volume = 2
					sound.Parent = game.Players.LocalPlayer.PlayerGui
					sound:Play()
					game.Debris:AddItem(sound,5)
				end
			end)
			v.BackgroundColor = BrickColor.new(bgc1va,bgc2va,bgc3va)
			v.TextColor = BrickColor.new(tc1va,tc2va,tc3va)
			coroutine.resume(coroutine.create(function()
				repeat wait() until savechange == true
				canclick = false
			end))
		end
	end
end)
walkspeed:GetPropertyChangedSignal("Text"):Connect(function()
	if walkspeed.Text == "WalkSpeed: Speed" then return end
	local ezr = false
	coroutine.resume(coroutine.create(function()
		while wait() do
			if ezr == true then break end
			if not plr.Character then return end
			if not plr.Character:FindFirstChild("Humanoid") then return end
			if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= walkspeed.Text then
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = walkspeed.Text
			end
		end
	end))
	walkspeed:GetPropertyChangedSignal("Text"):Connect(function()
		ezr = true
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	end)
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://9119720940"
	sound.Volume = 2
	sound.Parent = plr.PlayerGui
	sound:Play()
	game.Debris:AddItem(sound,5)
end)
