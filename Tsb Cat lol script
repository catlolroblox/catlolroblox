autododgehitbox2.Touched:Connect(function(hit)
	if close == true then return end
	if autododgetoggle == false then return end
	if autododgecd2 == true then return end
	if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= plr.Name then
		for i,v in pairs(hit.Parent:GetChildren()) do
		if v.Name:find("Counter") or v.ClassName == "ForceField" or v.Name == "Slowed" or v.Name == "Freeze" then
		if hit.Parent.Humanoid.FloorMaterial == Enum.Material.Air then return end	
		plr.Character.HumanoidRootPart.CFrame = hit.Parent.HumanoidRootPart.CFrame * CFrame.new(0,0,55)
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://5066021887"
		sound.Volume = 0.5
		sound.Parent = plr.PlayerGui
		sound:Play()
		game.Debris:AddItem(sound,5)
		coroutine.resume(coroutine.create(function()
		autododgecd2 = true
		task.wait(1)
		autododgecd2 = false
			end))
			end
			end
			for i,v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "GlowingArm" or v.Name == "HiddenFist" or v.Name == "HasKatana" or v.Name == "DoingBladeSkill" or v.Name == "GreenAura" or v.Name == "NoRotateUltimate" then
					if hit.Parent.Humanoid.FloorMaterial == Enum.Material.Air then return end	
					if v.Name == "GreenAura" then
						local vim = game:GetService("VirtualInputManager")
						vim:SendKeyEvent(true, Enum.KeyCode.F, false, nil)
						task.wait(1)
						vim:SendKeyEvent(false, Enum.KeyCode.F, false, nil)
					end		
					repeat wait(0.5)
					plr.Character.HumanoidRootPart.CFrame = hit.Parent.HumanoidRootPart.CFrame * CFrame.new(0,0,55)
					until not hit.Parent:FindFirstChild(v)
					local sound = Instance.new("Sound")
					sound.SoundId = "rbxassetid://5066021887"
					sound.Volume = 0.5
					sound.Parent = plr.PlayerGui
					sound:Play()
					game.Debris:AddItem(sound,5)
					coroutine.resume(coroutine.create(function()
						autododgecd2 = true
						task.wait(1)
						autododgecd2 = false
					end))
				end
			end
	end
	end)
