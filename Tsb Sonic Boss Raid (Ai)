local littletpcd = false

-- mouse vim:SendMouseButtonEvent(500, 500, 0, true, game, 1)
-- key vim:SendKeyEvent(true, Enum.KeyCode., false, nil)

function p()
	coroutine.resume(coroutine.create(function()
		while wait() do
			if close == true then break end
			local target = findNearestTorso(plr.Character.HumanoidRootPart.Position)
            if canattack == false then return end
			if enemychar then
				if enemychar:FindFirstChild("Humanoid") or not enemychar:FindFirstChild("HumanoidRootPart") then
					if not plr.Character:FindFirstChildOfClass("ForceField") and enemychar.Humanoid.Health > 0 and plr.Character.Humanoid.Health > 0 then
						
						local targetm1 = findNearestTorsoM1(plr.Character.HumanoidRootPart.Position)
						local targettooclose = findNearestTorsoTOOCLOSE(plr.Character.HumanoidRootPart.Position)
						local targetcandash = findNearestTorsoCANDASH(plr.Character.HumanoidRootPart.Position)
						local targetmidrange = findNearestTorsoMIDRANGE(plr.Character.HumanoidRootPart.Position)
						local targetveryclose = findNearestTorsoVERYCLOSE(plr.Character.HumanoidRootPart.Position)
						local targetmidrangeclose = findNearestTorsoMIDRANGECLOSE(plr.Character.HumanoidRootPart.Position)
						local targetcloserange = findNearestTorsoCLOSERANGE(plr.Character.HumanoidRootPart.Position)
						
						if targetmidrange ~= nil then
							MIDRANGE = true
						else
							MIDRANGE = false
						end
						
						if targetm1 ~= nil and awaken == false and not enemychar.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity") and not enemychar:FindFirstChild("Ragdoll") and dontm1yet == false and not enemychar:FindFirstChildOfClass("ForceField") then
							vim:SendMouseButtonEvent(500, 500, 0, true, game, 1)
							vim:SendMouseButtonEvent(500, 500, 0, false, game, 1)
							vim:SendKeyEvent(true, Enum.KeyCode.Q, false, nil)
							vim:SendKeyEvent(true, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.Q, false, nil)	
							plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(0,0,4)
						end
						
						if awaken == true and closerange == true and not enemychar.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity") and not enemychar:FindFirstChild("Ragdoll") and dontm1yet == false and not enemychar:FindFirstChildOfClass("ForceField") then
							vim:SendMouseButtonEvent(500, 500, 0, true, game, 1)
							vim:SendMouseButtonEvent(500, 500, 0, false, game, 1)
							vim:SendKeyEvent(true, Enum.KeyCode.Q, false, nil)
							vim:SendKeyEvent(true, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.Q, false, nil)	
							plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(0,0,4)
						end
						
						local k = math.random(1,750)
						
						if k == 1 then
						
						local r = math.random(1,5)

						if r == 1 then
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("Tryharder...","All")
						end
						if r == 2 then
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("Try to be cool? wrong person kid...","All")
						end
						if r == 3 then
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("Weak...","All")
						end
						if r == 4 then
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("That all you got?","All")
						end
						if r == 5 then
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("Pathetic...","All")
						end
							
						end
						
						if MIDRANGE == true and enemychar:FindFirstChild("CrabCamera") and detectedcrabuser == false then
							detectedcrabuser = true
							local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
							chatrem:FireServer("Imagine need crab?","All")
							vim:SendKeyEvent(true, Enum.KeyCode.G, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.G, false, nil)
						end
						
						if plr.Character.Humanoid.Health <= 30 then
							if plr.PlayerGui.ScreenGui.MagicHealth.Ult.Visible == true and ult == false then
								repeat wait(0.01)
									vim:SendKeyEvent(true, Enum.KeyCode.G, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.G, false, nil)
								until plr.PlayerGui.ScreenGui.MagicHealth.Ult.Visible == false
								ult = true
								local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
								local UMessage = " "
								local SPrefix = "[S]:"
								local SMessage = "Boss has Used The Ultimate!"
								local A_1   = UMessage.."                                                                                                                                              ".. SPrefix.. " ".. SMessage
								local A_2   = "All"

								chatrem:FireServer(A_1,A_2)
								
								local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
								chatrem:FireServer("THIS BATTLE SHALL LAST LONGER!!!","All")
								coroutine.resume(coroutine.create(function()
									wait(5)
									local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
									chatrem:FireServer("CAN YOU EVEN SEE ME","All")
								end))
								coroutine.resume(coroutine.create(function()
									wait(30)
									ult = false
								end))
							end			
						end
						
						if ult == true then
							plr.Character.Humanoid.WalkSpeed = 500
						end

						if plr.Character:FindFirstChild("Ragdoll") and awaken == true then
							plr.Character.HumanoidRootPart.CFrame *= CFrame.new(math.random(-250,250),0,math.random(-250,250))
							repeat wait() until not plr.Character:FindFirstChild("Ragdoll")
						end
						
						if target ~= nil and TOOCLOSE == false and dontgoyet == false then
							vim:SendKeyEvent(true, Enum.KeyCode.W, false, nil)
							vim:SendKeyEvent(true, Enum.KeyCode.D, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
							if MIDRANGE == true then
							vim:SendKeyEvent(true, Enum.KeyCode.Space, false, nil)	
							end
						end
						
						if normalspeed == true then
							plr.Character.Humanoid.WalkSpeed = 50
						end
						
						if awaken == true then
							normalspeed = false
							if ult == false then
								plr.Character.Humanoid.WalkSpeed = 125
							end
							if closerange == true and awakenm1cd == false then
								awakenm1cd = true
								coroutine.resume(coroutine.create(function()
									wait(10)
									awakenm1cd = false
								end))
								for i=1,25 do
									plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(math.random(-50,50),0,math.random(-50,50))
								   task.wait(0.01)
								end	
							end
						end
						
						for i,v in pairs(enemychar:GetChildren()) do
							if v.Name == "Counter" then
								dontgoyet = true
								dontm1yet = true
								canattack = false
								canlock = false
								counterchatcd = true
								local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
								chatrem:FireServer("Bro really tried to death counter me?","All")
								wait(1)
								repeat wait(0.5) until not enemychar:FindFirstChild("Counter")
								dontgoyet = false
								dontm1yet = false
								canattack = true
								canlock = true
							end
							if v.Name:find("Counter") then	
								if v.Name ~= "ProjectileCounter" then
								dontgoyet = true
								dontm1yet = true
								canattack = false
								canlock = false
								local oldhealth = plr.Character.Humanoid.Health
								repeat wait(0.5) until not enemychar:FindFirstChild(v)
								dontgoyet = false
								dontm1yet = false
								canattack = true
								canlock = true
