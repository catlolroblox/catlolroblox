							vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
							vim:SendKeyEvent(true, Enum.KeyCode.Two, false, nil)
							wait(0.1)
							vim:SendKeyEvent(false, Enum.KeyCode.Two, false, nil)
							coroutine.resume(coroutine.create(function()	
							dontm1yet = true		
							repeat task.wait() until closerange == true or enemychar:FindFirstChild("GreenAura") or enemychar:FindFirstChild("HasKatana") or enemychar:FindFirstChild("HiddenFist") or enemychar:FindFirstChild("Freeze") or enemychar:FindFirstChild("DoingBladeSkill") or enemychar.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity")
							dontm1yet = false		
							vim:SendKeyEvent(true, Enum.KeyCode.Two, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.Two, false, nil)
							end))
						end
						
						if ult == true and not plr.PlayerGui.Hotbar.Backpack.Hotbar["3"].Base:FindFirstChild("Cooldown") and not enemychar:FindFirstChild("Ragdoll") and canattack == true then
							vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
							vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
							vim:SendKeyEvent(true, Enum.KeyCode.Three, false, nil)
							vim:SendKeyEvent(false, Enum.KeyCode.Three, false, nil)
						end
						
						if enemychar:FindFirstChild("Freeze") or enemychar:FindFirstChild("Slowed") then
							if tpcd == false and awaken == false and not enemychar:FindFirstChildOfClass("ForceField") then
								coroutine.resume(coroutine.create(function()
									tpcd = true
									wait(5)
									tpcd = false
								end))
									plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(math.random(-30,30),0,math.random(-30,30))
							end
						end
						
						if enemychar:FindFirstChild("Freeze") or enemychar:FindFirstChild("Slowed") then
							if tpcd == false and awaken == true and not enemychar:FindFirstChildOfClass("ForceField") then
								coroutine.resume(coroutine.create(function()
									tpcd = true
									wait(1)
									tpcd = false
								end))
								plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(math.random(-30,30),0,math.random(-30,30))
							end
						end
						
						if enemychar.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity") or enemychar:FindFirstChild("Freeze") or enemychar:FindFirstChild("Slowed") then
								if not enemychar:FindFirstChildOfClass("ForceField") and canattack == true and tpcd == false then
									tpcd = true
									coroutine.resume(coroutine.create(function()
										tpcd = true
										if awaken == true then
											wait(1)
										end
										if awaken == false then
											wait(5)
										end
										tpcd = false
									end))
									plr.Character.HumanoidRootPart.CFrame = enemychar.HumanoidRootPart.CFrame * CFrame.new(math.random(-30,30),0,math.random(-30,30))
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["1"].Base:FindFirstChild("Cooldown") and MIDRANGE == true and ult == false then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.One, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.One, false, nil)
								end
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["3"].Base:FindFirstChild("Cooldown") and MIDRANGE == true and ult == false then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.Three, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.Three, false, nil)
								end
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["1"].Base:FindFirstChild("Cooldown") and MIDRANGE == true and ult == true then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.One, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.One, false, nil)
								end
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["2"].Base:FindFirstChild("Cooldown") and MIDRANGE == false and ult == true then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.Two, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.Two, false, nil)
									coroutine.resume(coroutine.create(function()
										repeat task.wait() until closerange == true
										vim:SendKeyEvent(true, Enum.KeyCode.Two, false, nil)
										vim:SendKeyEvent(false, Enum.KeyCode.Two, false, nil)
									end))
								end
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["3"].Base:FindFirstChild("Cooldown") and MIDRANGE == true and ult == true then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.Three, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.Three, false, nil)
								end
								if not plr.PlayerGui.Hotbar.Backpack.Hotbar["4"].Base:FindFirstChild("Cooldown") and closerange == true and ult == true then
									vim:SendKeyEvent(false, Enum.KeyCode.Space, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.W, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.A, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.D, false, nil)	
									vim:SendKeyEvent(false, Enum.KeyCode.S, false, nil)	
									vim:SendKeyEvent(true, Enum.KeyCode.Four, false, nil)
									vim:SendKeyEvent(false, Enum.KeyCode.Four, false, nil)
								end
							end
						end
							
						end
							
						if targetveryclose ~= nil then
							veryclose = true
						else
							veryclose = false
						end


					end	
				end
			end
		end
	end))
end

p()
