function getplayerva()
	for i,v in pairs(game.Players:GetChildren()) do
		if not v.Character:FindFirstChild("PlayerVa") then
		local va = Instance.new("BoolValue")
		va.Name = "PlayerVa"
		va.Parent = v.Character
		end
	end
end

getplayerva()

game.Workspace.Live.ChildAdded:Connect(function()
	getplayerva()
end)

game.Workspace.Live.ChildRemoved:Connect(function()
	getplayerva()
end)

