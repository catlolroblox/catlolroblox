	if not plr.Character:FindFirstChild("HumanoidRootPart") then return end
	local target = findNearestTorso(plr.Character.HumanoidRootPart.Position)
		if target ~= nil then
		if sideaim == true then
			Camera.CFrame = CFrame.lookAt(Camera.CFrame.Position+Vector3.new(0,2,0),target.Position)
		else
			Camera.CFrame = CFrame.lookAt(Camera.CFrame.Position+Vector3.new(0,2,0),target.Position)
		end
			
			if target.Parent:FindFirstChild("PlayerVa") then
				enemychar = target.Parent
			end
			
		if close == false then
			UserGameSettings.RotationType = Enum.RotationType.CameraRelative
		end
    end
	end
end)

